
class Student{
	int id;
	String name;
	float per;
	public void setId(int id) {
		this.id=id;
	}
	public int getId() {
		return id;
	}
	public void setName(String name) {
		this.name=name;
	}
	public getName() {
		return name;
	}
}
public class EncapsulationDemo {
	public static void main(String[] args) {
		
	}
}
